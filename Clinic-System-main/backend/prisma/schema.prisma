generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  id        Int      @id @default(autoincrement())
  serial    Int
  name      String
  type      String   // REGULAR | REPORT
  status    String   // WAITING | NEXT | RUNNING | COMPLETED | REPORT
  createdAt DateTime @default(now())
}


